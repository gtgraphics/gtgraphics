= image_tag 'prints_header.jpg', alt: page.title, class: 'img-responsive hidden-xs unselectable'

- if region_content_for? :main
  .page-body
    .row
      .col-sm-offset-1.col-sm-10.col-md-offset-2.col-md-8.text-highlight
        = yield_region :main

%hr

.page-body
  .row
    .col-sm-5.col-sm-offset-1.col-md-4.col-md-offset-1
      %h2.text-center Tobias Roetsch
      .row
        .col-xs-4
          = link_to 'http://www.taenaron.fineartprint.de', target: '_blank', class: 'icon-menu-link', title: 'Fineartprint', alt: 'Fineartprint', data: { toggle: 'tooltip', placement: 'top' } do
            = image_tag 'shops/shop-150/fineartprint.png', class: 'img-responsive'
            %span.sr-only Fineartprint
        .col-xs-4
          = link_to 'http://www.posterlounge.de/kuenstler/roetsch', target: '_blank', class: 'icon-menu-link', title: 'Posterlounge', alt: 'Posterlounge', data: { toggle: 'tooltip', placement: 'top' } do
            = image_tag 'shops/shop-150/posterlounge.png', class: 'img-responsive'
            %span.sr-only Posterlounge
        .col-xs-4
          = link_to 'http://taenaron.deviantart.com/prints', target: '_blank', class: 'icon-menu-link', title: 'DeviantArt', alt: 'DeviantArt', data: { toggle: 'tooltip', placement: 'top' } do
            = image_tag 'shops/shop-150/deviantart.png', class: 'img-responsive'
            %span.sr-only DeviantArt
      .row
        .col-xs-4
          = link_to 'http://www.artflakes.com/de/shop/taenaron', target: '_blank', class: 'icon-menu-link', title: 'Artflakes', alt: 'Artflakes', data: { toggle: 'tooltip', placement: 'top' } do
            = image_tag 'shops/shop-150/artflakes.png', class: 'img-responsive'
            %span.sr-only Artflakes
        .col-xs-4
          = link_to 'http://fineartamerica.com/profiles/tobias-roetsch.html', target: '_blank', class: 'icon-menu-link', title: 'FineArtAmerica', alt: 'FineArtAmerica', data: { toggle: 'tooltip', placement: 'top' } do
            = image_tag 'shops/shop-150/fineartamerica.png', class: 'img-responsive'
            %span.sr-only FineArtAmerica
        .col-xs-4
          = link_to 'http://society6.com/taenaron', target: '_blank', class: 'icon-menu-link', title: 'Society6', alt: 'Society6', data: { toggle: 'tooltip', placement: 'top' } do
            = image_tag 'shops/shop-150/society6.png', class: 'img-responsive'
            %span.sr-only Society6

    .col-sm-5.col-md-4.col-md-offset-2
      %h2.text-center Jeff Michelmann
      .row
        .col-xs-4
          = link_to '#', target: '_blank', class: 'icon-menu-link' do
            Bla

        .col-xs-4
          = link_to '#', target: '_blank', class: 'icon-menu-link' do
            Bla
